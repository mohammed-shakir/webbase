{
  "name": "webbase",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "scripts": {
    "docs:clean": "rimraf docs",
    "docs:generate": "npm run docs:clean && typedoc",
    "docs:serve": "npx live-server docs",
    "studio": "dotenv -e .env.local -- prisma studio",
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "npm-run-all -p test:unit-integration test:e2e",
    "test:unit-integration": "vitest run --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:watch": "vitest --ui"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.4.2",
    "@google-cloud/pino-logging-gcp-config": "^1.0.2",
    "@prisma/client": "^6.6.0",
    "@sentry/nextjs": "^9.13.0",
    "@types/swagger-ui-dist": "^3.30.5",
    "clsx": "^2.1.1",
    "helmet": "^8.1.0",
    "next": "15.3.1",
    "next-connect": "^0.12.2",
    "next-themes": "^0.4.6",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redoc": "^2.5.0",
    "swagger-ui-dist": "^5.21.0",
    "swagger-ui-themes": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/auth0": "^3.3.10",
    "@types/node": "22.14.1",
    "@types/react": "^19",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitest/ui": "^3.1.2",
    "autoprefixer": "^10.4.21",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "^26.1.0",
    "next-sitemap": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prisma": "^6.6.0",
    "swagger-jsdoc": "^6.2.8",
    "tailwindcss": "^4.1.4",
    "typedoc": "^0.28.3",
    "typescript": "^5",
    "vite": "^6.3.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.2"
  }
}
